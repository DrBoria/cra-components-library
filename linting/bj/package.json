{
  "name": "ag-blackjack",
  "version": "0.0.1",
  "description": "AG Blackjack",
  "keywords": [
    "game",
    "webpack",
    "typescript"
  ],
  "author": "AuthenticGaming",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AuthenticGaming/ag.ui.blackjack.git"
  },
  "homepage": "http://www.authenticgaming.com/",
  "scripts": {
    "clean-dist": "rm -f -r -d dist",
    "clean-docs": "rm -f -r -d docs",
    "start:devloader": "cross-env NODE_ENV=production node index.js",
    "start": "concurrently \"cross-env webpack-dev-server --config=configs/webpack/dev.js\" \"npm run gen:icons:watch\"",
    "build": "cross-env npm run clean-dist && webpack -p --config=configs/webpack/prod.js --env.PLATFORM=production",
    "check-typings": "tsc --noEmit",
    "docs": "npm run clean-docs & ./node_modules/.bin/jsdoc -c jsdoc.json",
    "gen:icons": "node ./gen-icons.js && npm run prettify -- \"$INIT_CWD/src/components/Icon/icons.tsx\"",
    "gen:icons:watch": "nodemon --exec \"npm run gen:icons\" --watch src/assets/images -e svg",
    "prettify": "prettier-eslint --write",
    "prettify:all": "prettier-eslint --write \"$INIT_CWD/src/**/*.{js,jsx,tsx,ts}\"",
    "lint:js": "eslint src/ --ext .jsx --ext .js",
    "lint:js:fix": "eslint --fix",
    "lint:css": "stylelint **/*.scss",
    "lint:css:fix": "npm run lint:css -- --fix",
    "test": "jest --silent -u --maxWorkers=8",
    "test:watch": "jest --watch",
    "lint-staged": "lint-staged",
    "test:watch-all": "jest --watchAll",
    "core:latest": "npm i @authenticgaming/ag.ui.core@latest",
    "core:link": "npm link @authenticgaming/ag.ui.core"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@testing-library/dom": "^7.29.4",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.2",
    "@trivago/prettier-plugin-sort-imports": "^1.4.1",
    "@types/classnames": "^2.2.11",
    "@types/file-saver": "^2.0.2",
    "@types/hls.js": "^0.13.3",
    "@types/howler": "^2.2.2",
    "@types/jest": "^25.2.3",
    "@types/node": "^13.13.5",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-redux": "^7.1.7",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "autoprefixer": "^9.8.6",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "better-docs": "^2.3.2",
    "concurrently": "^6.2.0",
    "copy-webpack-plugin": "^6.0.4",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "file-loader": "^6.0.0",
    "hls.js": "^1.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "jest-css-modules": "^2.1.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jsdoc": "^3.6.4",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.8",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.2.1",
    "prettier-eslint": "12.0.0",
    "prettier-eslint-cli": "5.0.0",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20",
    "redux-mock-store": "^1.5.4",
    "redux-observable": "^1.2.0",
    "sass-lint": "^1.13.1",
    "sass-loader": "^8.0.2",
    "snapshot-diff": "^0.8.1",
    "style-loader": "^1.1.4",
    "stylelint": "^13.11.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "ts-jest": "^26.1.0",
    "tslint": "^6.1.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-origin-ordered-imports-rule": "^1.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@authenticgaming/ag.ui.core": "^4.1.239",
    "@types/react-transition-group": "^4.4.1",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "file-saver": "^2.0.5",
    "howler": "^2.2.1",
    "i18next": "^19.4.4",
    "immutability-helper": "^3.0.2",
    "react": "^16.13.1",
    "react-i18next": "^11.4.0",
    "react-popper-tooltip": "^4.3.0",
    "react-redux": "^7.2.0",
    "react-svg-loader": "^3.0.3",
    "react-transition-group": "^4.4.1",
    "react-use": "^17.2.4",
    "redux": "^4.0.5",
    "reselect": "^4.0.0",
    "rxjs": "^6.5.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,tsx,ts}": [
      "prettier-eslint --write"
    ],
    "**/*.scss": [
      "npm run lint:css:fix"
    ]
  }
}
